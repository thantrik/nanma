{
  "background": {
    "scripts": ["onInstall.js"],
    "persist": false
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "description": "Web developer essential tools.",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "128": "icons/icon-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'unsafe-inline' data: blob: filesystem: https://raw.githubusercontent.com/*;  worker-src 'self' https: blob:; ",
  "manifest_version": 2,
  "name": "Web developer tool",
  "permissions": [
    "clipboardWrite",
    "<all_urls>",
    "contextMenus",
    "https://raw.githubusercontent.com/*"
  ],
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["static/js/bundle.js", "jsonViewer.js"],
      "css": ["static/css/style.css"],
      "run_at": "document_end"
    }
  ]
}
